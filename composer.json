{
    "name": "muffin/social-meta",
    "description": "Adds Facebook Open Graph and Twitter Cards support to CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "social",
        "facebook",
        "og",
        "open graph",
        "twitter"
    ],
    "homepage": "https://github.com/usemuffin/social-meta",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/social-meta/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/social-meta/issues",
        "source": "https://github.com/usemuffin/social-meta"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\SocialMeta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\SocialMeta\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
